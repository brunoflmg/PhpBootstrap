{
    "name": "jaimeneto/php-bootstrap",
    "description": "A bootstrap component creator for PHP",
    "keywords": ["php", "bootstrap"],
    "type": "library",
    "license": "BSD Style",
    "authors": [
        {
            "name": "Jaime da Costa Pereira Neto",
            "email": "contato@jaimeneto.com"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "PhpBootstrap\\": [
                "library/PhpBootstrap"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpBootstrap\\": [
                "tests/PhpBootstrap"
            ]
        }
    }
}
